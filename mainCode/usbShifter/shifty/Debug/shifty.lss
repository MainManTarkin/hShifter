0000c6 e091                      	ldi r25, hidDesriptorRegisterHigh
                                 
                                 	//store write register with address of hid descriptor
0000c7 9381                      	st z+, r24
0000c8 8390                      	st z, r25
                                 
                                 	//init error register to zero
0000c9 e2ee                      	ldi r30, hidErrorFlagLow
0000ca e0f1                      	ldi r31, hidErrorFlagHigh
                                 
0000cb 8210                      	st z, r1
                                 
